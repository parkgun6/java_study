package d4;

public class Study1 {
	public static void main(String[] args) {

		// 2018년 8월의날씨
		double[] date = { 40, 38, 38, 35, 34, 35, 36, 45, 33, 37 };
		double sum = 0;
		//루프를 돌아서 내용물을 출력
		for (int i = 0; i < date.length; i++) {
			System.out.println(date[i]);
			sum=sum+date[i];
		}

		System.out.println(sum);

		// 전체의 합
		// 전체의 평균
		// 제일 높은 수를 구하기
		
		// 제일 높은 수와 인덱스 번호 구하기
	}
}