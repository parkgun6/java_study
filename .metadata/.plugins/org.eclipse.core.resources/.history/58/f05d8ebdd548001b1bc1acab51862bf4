package d5;

import java.util.Scanner;

public class Game {

	public static String showName(int num) {
		String result = "";// 출력이 문자열이기때문에 String
		switch (num) {
		case 0:
			result = "가위";
			break;
		case 1:
			result = "바위";
			break;
		case 2:
			result = "보자기";
			break;
		}

		return result;
	}

	public static void main(String[] args) {
		Scanner scanner = new Scanner(System.in);
		System.out.println("가위는0 바위는1 보자기는 2");
		int user = scanner.nextInt();

		int com = (int) (Math.random() * 3);

	}
}
