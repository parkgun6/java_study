package d5;

public class Study2 {
	public char getResult(double indexValue) {
		double idexValue = 0;
		if (idexValue > 30) {
			return 'A';
		} else if (idexValue > 24) {
			return 'B';
		} else if (idexValue > 20) {
			return 'C';
		} else if (indexValue > 15) {
			return 'D';
		} else if (indexValue > 13) {
			return 'E';
		} else if (indexValue > 10) {
			return 'F';
		} else {
			return 'G';
		}
	}

	public double Calculate(double height, double weight) {

		double hData = Math.pow(height, 2);
		double result = weight / hdata;
		return result;
	}

	public static void main(String[] args) {
		Study2 m = new Study2();
		double h = 1.8D;
		double w = 70D;
		double index = Calculate(h, w);

	}
}
